{
    "manifest_version": 3,
    "name": "Tampilkan Elemen Tersembunyi",
    "version": "1.0",
    "description": "Menampilkan elemen yang sebelumnya disembunyikan (display:none)",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "content.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
  "default_title": "Tampilkan Elemen Tersembunyi"
}

  }
  